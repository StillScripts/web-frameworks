{
  "name": "qwik",
  "description": "Starter with built-in routing, powered by Qwik City Vite.js tooling. Eslint code linting. Prettier code formatter.",
  "scripts": {
    "build": "npm run typecheck && npm run build.client && npm run build.ssr",
    "build.client": "vite build",
    "build.ssr": "vite build --ssr src/entry.server.tsx",
    "dev": "vite",
    "dev.debug": "node --inspect-brk node_modules/vite/bin/vite.js",
    "dev.ssr": "vite --mode ssr",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"**/*.ts*\"",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@builder.io/qwik": "0.0.24",
    "@builder.io/qwik-city": "0.0.12",
    "@types/eslint": "8.4.2",
    "@types/node": "latest",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-plugin-qwik": "latest",
    "node-fetch": "2.6.7",
    "prettier": "2.6.2",
    "typescript": "4.7.2",
    "vite": "2.9.9"
  },
  "engines": {
    "node": ">=14"
  },
  "homepage": "https://qwik.builder.io/",
  "license": "",
  "private": true
}
